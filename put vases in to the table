package com.company;

public class Main{

    public static void main(String args[]){
        int n=10;
        int dane[]=new int[n];

        for(int i=0; i<10 ; i++){
            --n;
            dane[i]=n;
            System.out.println("dane[" + i + "]=" + dane[i]);

        }
        System.out.println(dane.length);

    }
}
