package com.company;

public class Main{

    public static void main(String args[]){
        int suma=0;
        int macierz[][]=new int[10][10];
        for(int i=0 ; i<macierz.length ; i++){
            for(int j=0 ; j<macierz.length ; j++){
               if(i==j){
                   macierz[i][j]=1;
                   suma+=macierz[i][j];

               }
               else {
                   macierz[i][j]=0;
               }
               System.out.print(macierz[i][j]+" ");
            }
            System.out.println();
        }
        System.out.print("suma" + suma);
    }
}
