package com.company;
import java.util.*;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        int x=0;
        int y=0;

        do{
           y=3*x;
           System.out.println("x=" + x +'\t' + "y=" + y);
        x++;
        }while(x<=10);
    }
}
